		}
		d = d.replace ( /^prefix.+$/mig , '' ) ;
		d = d.replace ( /\s+/g , ' ' ) ;
		d = $.trim ( d ) ;
		$(sparql_selector).val ( d ) ;
		$(wdq_selector).val ( '' ) ;
	} ) ;
	
	return false ;
}

