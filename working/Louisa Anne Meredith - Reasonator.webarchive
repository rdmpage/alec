	if ( me.ns != 14 ) return ;

	if ( o.cacheable ) {
		var cacheKey = [ o.depth , me.lang , me.project , me.ns , me.title ] ;
		o.cacheKey = cacheKey.join ( '|' ) ;
		if ( undefined !== wikiDataCache.pagesInCategoryTree[o.cacheKey] ) {
			o.callback ( clone ( wikiDataCache.pagesInCategoryTree[o.cacheKey] ) ) ;
			return ;
		}
