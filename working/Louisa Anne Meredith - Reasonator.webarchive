.aliases {
	color: #999999;
	margin-bottom: 5px;
	max-height: 100px;
	overflow: auto;
}

.entity {
	display: none;
	margin-left: 5px;
}

#main_content {
	margin-top: 20px;
	box-shadow: 8px 8px 8px #DDD;
	margin-bottom: 20px;
	border-radius: 15px;
/*	padding-top: 5px;
	padding-bottom: 5px;
	padding-left: 2px;
	padding-right: 2px;*/
	background-color: #FFF;
	padding: 1px;
}

#top {
	margin-left: 5px;
}

div.header {
	margin-top: -12px;
}

h1.main_title {
	border-bottom: 1px solid #DDD;
	margin-top: 0px;
}

h1, h2, h3, h4, h5, h6 {
	color: #444;
	font-family: times new roman;
}

.sidebar {
	margin-bottom: 10px;
	border-left: 1px solid #DDD;
	padding-left: 5px;
}

.mainbar { }

#language_select {
    background-color: transparent;
    background-repeat: no-repeat;
    background-attachment: scroll;
    background-position: left center;
    background-clip: border-box;
    background-origin: padding-box;
    background-size: auto auto;
    background-image: linear-gradient(transparent, transparent), url('data:image/svg+xml;base64,');
	padding-left: 30px;
}

/* Cluetip CSS */

h3.ui-cluetip-header {
	line-height: 100%;
	font-size: 1.5em;
	padding: 2px;
	padding-left: 10px;
}

.cluetip-inner div {
	margin-bottom: 3px;
}

.main_image, .main_coa, .main_seal, .main_video, .main_audio, .signature, .main_range_map, .main_flag_image {
	text-align: center;
}

.main_image img, .main_coa img, .main_seal img, .main_video img, .main_audio img, .signature img, .main_range_map img, .main_flag_image img {
	margin-bottom: 1em;
}

div.cal_sb_header {
	text-align: center;
	font-weight: bold;
	background-color: #EEE;
}

div.cal_sb_section {
	margin-left: 10px;
}

span.inline_desc {
	font-size: 0.8em;
	color: #666;
}

table.person_relatives > tbody > tr > td {
	min-width: 30%;
}

div.personal_relations {
	display: none;
}

.missing_label {
	border-bottom: 1px dotted red;
}

.main_wikivoyage_banner {
	overflow:hidden;
}


/* Collapsible panels */
.panel-heading a:after {
    font-family: 'Glyphicons Halflings';
    content: "\e114";    
    float: right; 
    color: grey; 
}
.panel-heading a.collapsed:after {
    content: "\e080";
}

div.youtube {
	background-size:contain;
	height:200px;
	background-repeat: no-repeat;
	cursor:pointer;
	margin-bottom:5px;
}

img.proprietary_logo {
	float:right;
	padding-right:10px;
	padding-top:5px;
