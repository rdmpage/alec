function prettyNumber(nStr)
{
	nStr += '';
	x = nStr.split('.');
	x1 = x[0];
	x2 = x.length > 1 ? '.' + x[1] : '';
	var rgx = /(\d+)(\d{3})/;
	while (rgx.test(x1)) {
		x1 = x1.replace(rgx, '$1' + ',' + '$2');
	}
	return x1 + x2;
}

function prettyTime ( seconds ) {
	seconds = Math.floor ( seconds ) ;
	var ret ;
	if ( seconds < 60 ) {
		ret = seconds + ' sec' ;
	} else {
		var min = Math.floor ( seconds / 60 ) ;
		ret = min + ' min ' + ( seconds - min*60 ) + ' sec' ;
	}
	return ret ;
}

