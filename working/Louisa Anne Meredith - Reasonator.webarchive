		alert ( "Please enter a WDQ string to convert!" ) ;
		return false ;
	}
	
	$(wdq_selector).prop('disabled', true);
	$(sparql_selector).prop('disabled', true);
	
	$.get ( '/wdq2sparql/w2s.php' , {
		wdq:wdq
	} , function ( d ) {
		$(wdq_selector).prop('disabled', false);
		$(sparql_selector).prop('disabled', false);
		if ( d.match ( /^<!DOCTYPE/ ) ) {
