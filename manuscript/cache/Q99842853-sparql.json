SPARQL-QUERY: queryStr=PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
SELECT ?o ?p WHERE
{ 
  VALUES ?work { wd:Q99842853 }
  
  {    
    # outbound
    ?work ?p ?o 
  }
  UNION
  {
    # inbound
      ?o ?p ?work .   
  }
  UNION
  {
  	  VALUES ?p { wdt:P248 }
      ?provenance pr:P248 ?work . 
      ?statement prov:wasDerivedFrom ?provenance .
 	  ?o p:P225 ?statement . 
   
  }
  # links to things
  ?o wdt:P31 ?type .
 }
